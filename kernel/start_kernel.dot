	do_basic_setup->do_initcalls

	init_post->"run_init_process(/init)"

	do_initcalls->do_one_initcall

	do_one_initcall[label="do_one_initcall(*fn)\nfor (fn = __early_initcall_end;\n fn < __initcall_end; fn++)"]
